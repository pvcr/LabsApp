<UserControl x:Class="Labs.Views.WorkPage.WorkPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Labs.Views.WorkPage"
             xmlns:local1="clr-namespace:Labs.Views.WorkTopPage"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="layoutWorkPageRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.7*"  />
            <RowDefinition Height="0.3*"  />
        </Grid.RowDefinitions>
      
        <local1:WorkTopPage Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"  />
        

    </Grid>
</UserControl>
